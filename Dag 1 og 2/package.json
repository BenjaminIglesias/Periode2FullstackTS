  "nodemonConfig": {
    "ignore": [
      "test/**/*",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "node -r ts-node/register"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "ts-node": "^9.1.1"
  }
}
