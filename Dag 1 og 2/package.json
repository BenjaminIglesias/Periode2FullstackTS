{
  "name": "FullstackStartcode",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node build/bin/www.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/bin/www.ts",
    "dev:debug": "nodemon --inspect src/bin/www.ts"
